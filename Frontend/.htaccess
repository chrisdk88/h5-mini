RewriteEngine On

# Block access to:

RewriteRule ^User/?$ - [F,L]
RewriteRule ^templates/?$ - [F,L]
RewriteRule ^Images/?$ - [F,L]
RewriteRule ^Admin/?$ - [F,L]
RewriteRule ^Admin/Upload/?$ - [F,L]

# Pretty URLs
RewriteRule ^login$ user/login.php [L]
RewriteRule ^signup$ user/signup.php [L]
RewriteRule ^dashboard$ user/dashboard.php [L]
RewriteRule ^edit-profile$ user/edit-profile.php [L]
RewriteRule ^statistic$ user/statistic.php [L]
